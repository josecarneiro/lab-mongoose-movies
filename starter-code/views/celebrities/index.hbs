<h2>Celebrities</h2>
{{#each celebrities}}
<a href="/celebrities/{{_id}}"> <h4>{{name}}</h4></a>
 <form action="/celebrities/{{_id}}/delete" method="POST"><button>Delete</button></form>
 <form action="/celebrities/{{_id}}/edit" method="GET"><button>Edit</button></form>
{{/each}}

<a href="/celebrities/new">Create New Celebrity</a>